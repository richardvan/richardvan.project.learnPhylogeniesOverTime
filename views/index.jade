script(src="/javascripts/bundle.js")

extends layout

block content
  h2  Species List
  p trying to display #{specimens}
  each specimen, i in specimens
    div
      pre Common Name:
          b #{specimen._common_name}
      pre         Phylum: #{specimen._classification_phylum}
      if (specimen._classification_subphylum)
        = div
          pre         Subphylum: #{specimen._classification_subphylum}
      pre           Genus: #{specimen._classification_genus}
  h2 Phylocanvas Quickstart (initialization code from website)
  #phylocanvas
  script(type='application/javascript', src='https://cdn.rawgit.com/phylocanvas/phylocanvas-quickstart/v2.7.2/phylocanvas-quickstart.js')
  script(type='application/javascript').
    (function (Phylocanvas) {
      var tree = Phylocanvas.createTree('phylocanvas',{zoom: 0.25, disableZoom:false, showInternalNodeLabels: true});
